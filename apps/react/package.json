{
  "name": "dashboard-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun with-env vite",
    "build": "tsc && bun with-env vite build",
    "preview": "bun with-env vite preview",
    "pre-commit": "bun lint-staged",
    "ladle:dev": "bun with-env ladle serve",
    "ladle:build": "bun with-env ladle build",
    "ladle:preview": "bun with-env ladle preview",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@hookform/resolvers": "^3.3.2",
    "@ladle/react": "4.0.0",
    "@mantine/hooks": "^7.3.1",
    "@nextui-org/react": "^2.1.13",
    "@tanstack/react-query": "4.35.3",
    "@trpc/client": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@vitejs/plugin-react": "^4.2.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "delay": "^6.0.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.4",
    "immer": "^10.0.3",
    "lucide-react": "^0.293.0",
    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "superjson": "^2.2.1",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "latest",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.4"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "biome check --apply .",
      "biome format --write ."
    ]
  }
}